$nothing
  _{MsgKeineEinsendung}
<h2>_{MsgEinsendungBewertungAlt}
$maybe bewertung <- mbewertung
  #{bewertung}
$nothing
  _{MsgKeineBewertung}
$if istTutor
  <h2>_{MsgEinsendungBewerten}
  <form role="form" action=@{EinsendungR aufgabeId studentId} method="post" enctype=#{formEnctype}>
    ^{formWidget}
